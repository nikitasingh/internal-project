<div id="year_list">
<table>
  <tr>
    <th>Name</th>
    <th>Year</th>
    <th>Zone</th>
   
  </tr>

<% @decorations.each do |dec| %>
<% @zone=Zone.find(dec.zone_id) %>
  <tr>
    <td><%= dec.name %></td>
    <td><%= dec.year %></td>
    <td><%= @zone.name %></td>
    
  </tr>
<% end %>
</table>